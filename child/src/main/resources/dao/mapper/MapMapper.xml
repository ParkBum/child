<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.mapper.MapMapper">
	<select id="gulist" resultType="String">
		select gu from seoul group by gu
	</select>
	<select id="lists" resultType="Integer">
		select count(gu) from seoul group by gu
	</select>
	<select id="getpoints" resultType="String" parameterType="map">
		select addr from seoul where gu = "${gu}"
		<if test="type != null">
			and type = "${type}"
		</if>
		<if test="bus != null">
			and bus = "${bus}"
		</if>
	</select>
	<select id="list" resultType="Daycare">
		select * from seoul2
	</select>
</mapper>