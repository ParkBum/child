<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.mapper.MapMapper">
	<select id="gulist" resultType="String">
		select gu from seoul2 group by gu
	</select>
	<select id="lists" resultType="Integer">
		select count(gu) from seoul2 group by gu
	</select>
	<select id="list" resultType="Daycare" parameterType="map">
		select * from seoul2 where gu = "${gu}"
		<if test="type != null or type != ''">
			and type = "${type}"
		</if>
		<if test="bus != null or bus != ''">
			and bus = "${bus}"
		</if>
		<!-- <if test="word != null && word != ''">
			and name like "%${word}%"
		</if> -->
	</select>
	<!-- <select id="list" resultType="Daycare">
		select * from seoul2
	</select> -->
</mapper>